{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["ES2021"],
    "types": ["node"],
    "strict": true,
    "noEmit": true,
    "downlevelIteration": true,
    "noUncheckedIndexedAccess": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
